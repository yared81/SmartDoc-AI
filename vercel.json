{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "requirementsTxt": "requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}
